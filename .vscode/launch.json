{
    // Rob added these
  	"version": "0.2.0",
 	"configurations": [
 		{
 			"name": "Launch",
 			"type": "node",
  			"request": "launch",
  			"program": "${workspaceRoot}/keystone.js",
  			"stopOnEntry": false,
 			"args": [],
 			"cwd": "${workspaceRoot}",
 			"runtimeExecutable": null,
 			"runtimeArgs": [
 				"--nolazy"
 			],
 			"env": {
 				"NODE_ENV": "development"
 			},
 			"console": "internalConsole",
 			"sourceMaps": false,
 			"outFiles": null
 		},
 		{
 			"name": "Attach",
 			"type": "node",
 			"request": "attach",
 			"port": 5858,
			"restart": true,
 			"sourceMaps": false,
 			"outDir": null,
 			"localRoot": "${workspaceRoot}",
 			"remoteRoot": null
 		}
 	]
 }